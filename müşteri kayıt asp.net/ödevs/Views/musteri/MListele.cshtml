@model IEnumerable<ödevs.Models.musteri>

@{
    ViewBag.Title = "MListele";
}

<h2>@ViewData["Baslik"]</h2>

<p>
    @ViewBag.Altbilgi
    @Html.ActionLink("Yeni Kayıt", "YeniKayit")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Ad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SoyAd)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Dogtar)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TCkimlikno)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Adres)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Ad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SoyAd)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dogtar)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TCkimlikno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Adres)
            </td>
            <td>
                @Html.ActionLink("Düzenle", "Duzenle", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Sil", "Sil", new { id = item.Id })
        </tr>
    }

</table>